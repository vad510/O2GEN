<script type="text/javascript">
    $.noConflict();
    jQuery(document).ready(function ($) {
        $('#datetimepicker2').datetimepicker({
            defaultDate: moment().subtract(2, 'days'),
            locale: 'ru',
            icons: {
                time: "fa fa-clock",
                date: "fa fa-calendar-alt",
                up: "fa fa-arrow-up",
                down: "fa fa-arrow-down"
            },
            maxDate: moment()
        });


    });
</script>



